{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "simple-logo-gallery/logo-gallery",
	"version": "1.0.1",
	"title": "Simple Logo Gallery",
	"category": "media",
	"description": "Display a row of logos normalized for consistent visual weight.",
	"keywords": [ "logo", "gallery", "logos", "partners", "clients" ],
	"textdomain": "simple-logo-gallery",
	"attributes": {
		"logos": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"id": { "type": "number" },
					"url": { "type": "string" },
					"alt": { "type": "string", "default": "" }
				}
			}
		},
		"gap": {
			"type": "number",
			"default": 16
		},
		"baseSize": {
			"type": "number",
			"default": 48
		}
	},
	"supports": {
		"html": false,
		"align": [ "wide", "full" ],
		"filter": {
			"duotone": true
		}
	},
	"editorScript": "file:./editor.js",
	"viewScript": "file:./view.js"
}
